{
  "name": "raj-browser",
  "version": "2.0.5",
  "description": "A UI and privacy focussed browser for the web from the web.",
  "main": "./out/main/index.js",
  "author": "Rajaniraiyn",
  "homepage": "https://github.com/Rajaniraiyn/raj-browser",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck": "tsc --noEmit",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.json",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "tsc --noEmit && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:current": "npm run build && electron-builder --config",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config",
    "prepare": "husky install"
  },
  "dependencies": {
    "@cliqz/adblocker-electron": "^1.25.1",
    "@electron-toolkit/preload": "^1.0.2",
    "@electron-toolkit/utils": "^1.0.2",
    "electron-context-menu": "^3.6.0",
    "electron-dl": "^3.4.1",
    "electron-updater": "^5.3.0",
    "node-fetch": "^3.2.10",
    "normalize-url": "^7.2.0",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@sveltejs/vite-plugin-svelte": "^1.1.0",
    "@types/node": "18.11.7",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "cz-conventional-changelog": "3.3.0",
    "electron": "^22.0.2",
    "electron-builder": "^24.0.0",
    "electron-notarize": "^1.2.2",
    "electron-vite": "^1.0.11",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "svelte": "^3.52.0",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.7",
    "vite-plugin-webfont-dl": "^3.4.0"
  },
  "packageManager": "yarn@3.2.3"
}
